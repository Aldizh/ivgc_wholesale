<table>
	<thead>
		<th>Login</th>
		<th>Password</th>
		<th>Openning Balance</th>
		<th>email</th>
		<th>ID</th>
		<th></th>
	</thead>
	<tbody>
		<% @result["account_list"].each do |e|%>
		<tr>
			<td><%= e["login"] %> </td>
			<td><%= e["password"] %> </td>
			<td><%= e["opening_balance"] %> </td>
			<td><%= e["subscriber_email"] %> </td>
			<td><%= e["id"] %> </td>
			<td><%= link_to "Details", :i_account => e["i_account"], :action => "accountInfo" %></td> 
			<td><%= button_to "Terminate", {:i_account => e["i_account"], :action => "accountTerminate"}, :confirm => "Are you sure?" %></td>
		</tr>
		<% end %>
	</tbody>
</table>