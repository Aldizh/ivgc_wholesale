<div class="slider-wrapper theme-default">
  <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-40480808-9', 'ivgc.net');
      ga('send', 'pageview');

  </script>
  <div id="slider" class="nivoSlider">
    <%= image_tag("slide1.jpg") %>
    <%= image_tag("slide2.jpg") %>
    <%= image_tag("slide3.jpg") %>
    <%= image_tag("slide4.jpg") %>
  </div>
</div>
<div class="left_section" id="left_section_home">
  <section>
    <header style="text-align:justify;font-size:17px;"><%= t "home.index.voip" %></header>
    <ul>
      <li><%= t ".vline1" %></li>
      <li><%= t ".vline2" %></li>
      <li><%= t ".vline3" %></li>
    </ul>
    <% if !session[:current_login] %>
      <article><%= link_to t(".sign_up_res"), voip_resellers_path %></article>
    <% end %>
  </section>
</div>

<div class="right_section" id="right_section_home">
  <section>
    <header style="text-align:justify;font-size:17px;"><%= t "home.index.wholesale" %></header>
    <ul>
      <li><%= t ".wline1" %></li>
      <li><%= t ".wline2" %></li>
      <li><%= t ".wline3" %></li>
    </ul>
    <% if !session[:current_login] %>
      <article><!--%= link_to t(".sign_up_whole"), signups_path %--><%= link_to t(".sign_up_whole"), voip_resellers_path %></article>
    <% end %>
  </section>
</div>
<%= javascript_include_tag "olark_contact_us" %>