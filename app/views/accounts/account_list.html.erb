<%= link_to "Go back to accounts", '/accounts' %>
<table>
	<thead>
		<th>ID</th>
		<th>Login</th>
		<th>Openning Balance</th>
		<th></th>
	</thead>
	<tbody>
		<% @result["account_list"].each do |e|%>
		<tr>
			<td><%= e["id"] %> </td>
			<td><%= e["login"] %> </td>
			<td><%= e["opening_balance"] %> </td>
			<td><%#= link_to "Details", :id => e["id"], :action => "get_account_info" %></td>
		</tr>
        <% end %>
     </tbody>
</table>